    "# exemplo de fatorial: 1*2*3*4....*10\n",
    "def fatorial(n):\n",
    "    prod = 1\n",
    "    for i in range(1,n+1,1):\n",
    "        prod = prod*i\n",
    "    return prod\n",
    "    \n",
    "a = 4\n",
    "b = fatorial(a)\n",
    "print(b)"
   ]
