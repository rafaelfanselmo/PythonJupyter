    "lista1 = ['a','g','t','c']\n",
    "lista2 = ['g','c','t','a']\n",
    "lista3 = []\n",
    "\n",
    "i = 0\n",
    "j = 0\n",
    "k = 0\n",
    "n = 2*len(lista1)\n",
    "\n",
    "while(i < n):\n",
    "    if(i % 2 == 0):\n",
    "        lista3.append(lista1[j])\n",
    "        j += 1\n",
    "    else:\n",
    "        lista3.append(lista2[k])\n",
    "        k += 1\n",
    "    i += 1\n",
    "    \n",
    "print(lista3)"
